/* pages/shop/shop.wxss */
/*shop head*/
.shop-head{
  padding-top:20rpx;
  width:750rpx;
  height:150rpx;
  background:#dedede;
}
.shop-head image{
  display:block;
  margin:0 auto;
  width:80rpx;
  height:80rpx;
  border:2rpx solid #bebcb7;
  border-radius:50%;
  background-color:#fff;
}
.shop-head .shop-name{
  display:block;
  width:750rpx;
  height:52rpx;
  line-height:52rpx;
  font-size:32rpx;
  font-weight:bold;
  text-align:center;
  color:#fff;
}
/*goods wrapper*/
.goods-wrapper{
  width:750rpx;
}
.goods-wrapper .wrapper-title{
  width:750rpx;
  height:74rpx;
  border-bottom:2rpx solid #e5e7e2;
}
.wrapper-title text{
  float:left;
  margin-left:30rpx;
  margin-top:20rpx;
  height:50rpx;
  line-height:50rpx;
  font-size:32rpx;
  color:#26b1fe;
  font-weight:bold;
  border-bottom:5rpx solid #26b1fe;
}
/*good*/
.car-info-head .goods{
  padding-top:80rpx;
}
.good{
  position:relative;
  height:195rpx;
  border-bottom:2rpx solid #e5e7e2;
}
.good image{
  float:left;
  margin-top:22rpx;
  margin-left:28rpx;
  margin-right:22rpx;
  width:145rpx;
  height:145rpx;
  border:2rpx solid #e5e7e2;
  border-radius:10rpx;
}
.good .good-info{
  float:left;
  width:550rpx;
}
.good .good-info text{
  display:block;
}
.good .good-info .good-name{
  margin-top:22rpx;
  line-height:42rpx;
  font-size:30rpx;
  width:520rpx;
  height:90rpx;
  color:#000;
  word-break:break-all;
  overflow:hidden;
}
.car-info-wrapper .good .good-info .good-name{
  width:500rpx;
}
.good .good-info .good-price{
  margin-top:18rpx;
  height:48rpx;
  font-size:28rpx;
  line-height:48rpx;
  color:#fa3e2d;
}
.good .add-sub{
  position:absolute;
  right:30rpx;
  bottom:28rpx;
}
.good .add-sub text{
  float:left;
  font-size:34rpx;
}
.good .add-sub .add,
.good .add-sub .sub{
  margin:0;
  float:left;
  width:50rpx;
  height:50rpx;
  border-radius:50%;
}
.ex-tip{/*失效*/
  float:left;
  margin-top:22rpx;
  margin-left:15rpx;
  width:70rpx;
  height:30rpx;
  font-size:24rpx;
  line-height:30rpx;
  text-align:center;
  border-radius:10rpx;
  background:#ccc;
  color:#fff;
}
.good .add-sub .num{
  width:60rpx;
  height:50rpx;
  line-height:50rpx;
  color:#333;
  text-align:center;
}
.good.expired .add-sub .num{
  margin-right:50rpx;
}
.nogood{
  margin-top:400rpx;
  width:750rpx;
  height:32rpx;
  line-height:32rpx;
  text-align:center;
  font-size:30rpx;
  color:#8f8f8f;
}
/*底部购物车*/
.car-bar{
  position:fixed;
  left:0;
  bottom:0;
  width:750rpx;
  height:86rpx;
  border-top:1rpx solid #eee;
  background:#fff;
}
.car-icon{
  position:absolute;
  left:20rpx;
  top:-10rpx;
  width:90rpx;
  height:90rpx;
  background:#999;
  text-align:center;
  border-radius:50%;
  box-shadow: rgba(0,0,0,0.2) 0 -1rpx 10rpx;
}
.car-icon .car-num{
  position:absolute;
  left:60rpx;
  top:-10rpx;
  background:#f12c20;
  height:40rpx;
  width:40rpx;
  line-height:40rpx;
  color:#fff;
  font-size:20rpx;
  text-align:center;
  border-radius:50%;
}
.car-icon image{
  display:inline-block;
  margin-top:20rpx;
  width:56rpx;
  height:56rpx;
}
.car-icon.active{
  background:#26b1fe;
}
.car-info-wrapper .car-icon{
  top:-100rpx;
}
.car-money{
  margin-left:120rpx;
  float:left;
  width:440rpx;
}
.car-money .car-total-money,
.car-money .delivery-money{
  display:block;
  color:#000;
}
.car-money .car-total-money text{
  color:#f33;
}
.car-money .car-total-money{
  margin-top:10rpx;
  height:40rpx;
  line-height:40rpx;
  font-size:30rpx;
  font-weight:bold;
}
.car-money .delivery-money{
  height:34rpx;
  line-height:34rpx;
  font-size:24rpx;
}
.to-pay{
  float:right;
  width:190rpx;
  height:86rpx;
  line-height:86rpx;
  text-align:center;
  font-size:30rpx;
  font-weight:bold;
  color:#fff;
  background:#26b1fe;
}
.to-pay.disabled{
  background:#999;
}
/*购物车详情*/
.car-info-wrapper{
  position:fixed;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background:rgba(0,0,0,.3);
}
.car-info-content{
  position:absolute;
  left:0;
  bottom:86rpx;
  height:750rpx;
  background:#fff;
}
.car-info-wrapper .scrollview{
  top:80rpx;
}
.car-info-wrapper .car-bar .car-money{
  margin-left:30rpx;
}
.car-info-wrapper image{
  margin-left:15rpx;
}
.car-info-wrapper .car-icon image{
  margin-left:0;
}
.car-info-wrapper .good .good-info{
  width:470rpx;
}
.car-info-head{
  width:750rpx;
  height:80rpx;
  background:#fff;
  border-bottom:2rpx solid #e5e7e2
}
.car-info-head .select{
  float:left;
  width:400rpx;
}
.select-icon{
  float:left;
  width:36rpx;
  height:36rpx;
  border-radius:50%;
  border:2rpx solid #26b1fe;
}
.select-icon image{
  display:none;
  float:left;
  margin-top:6rpx;
  margin-left:5rpx;
  width:30rpx;
  height:26rpx;
  border:0;
}
.select .select-icon image{
  margin-left:3rpx;
}
.select-icon.active image{
  display:block;
}
.select-icon.active{
  background:#26b1fe;
}
.good .select-icon{
  margin-left:26rpx;
  margin-top:77rpx;
}
.car-info-head .select .select-icon{
  margin-left:29rpx;
  margin-top:25rpx;
  margin-right:10rpx;
}
.car-info-head .select .has-selected{
  float:left;
  margin-top:8rpx;
  height:72rpx;
  line-height:72rpx;
  font-size:28rpx;
  color:#000;
}
.car-info-head .select .has-selected text{
  color:#666;
}
/*清空购物车*/
.car-info-head .clear-car{
  float:right;
  width:220rpx;
  height:80rpx;
  margin-right:30rpx;
}
.car-info-head .clear-car image{
  float:left;
  margin-top:26rpx;
  width:34rpx;
  height:34rpx;
}
.car-info-head .clear-car text{
  float:left;
  padding-left:10rpx;
  margin-top:10rpx;
  height:70rpx;
  line-height:70rpx;
  color:#666;
  font-size:30rpx;
}
/*领取优惠券*/
.receive-coupon-wrapper{
  position:fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background:rgba(0,0,0,.5);
}
.receive-coupon-wrapper .receive-coupon{
  position:absolute;
  left:50%;
  top:40%;
  width:530rpx;
  height:600rpx;
  transform:translate(-50%,-50%);
  background:#fff;
  background-size:530rpx 230rpx;
  border-radius:16rpx;
}
.receive-coupon .img-wrapper{
  float:left;
  width:530rpx;
  height:230rpx;
  background:rgb(33,179,254);
  border-radius:16rpx;
}
.receive-coupon .img-wrapper image{
  float:left;
  width:100%;
  height:100%;
  border-radius:16rpx;
}
.receive-coupon .receive-coupon-info{
  position:relative;
  margin:290rpx auto 0;
  width: 480rpx;
  height:130rpx;
  background-size:480rpx 130rpx;
  color:#fff;
  box-sizing: border-box;
}
.receive-coupon-info-back{
  position:absolute;
  top:290rpx;
  left:50%;
  width:480rpx;
  height:130rpx;
  transform:translateX(-50%);
}
.receive-coupon .receive-coupon-info .receive-coupon-info-left{
  position:absolute;
  top:50%;
  transform:translateY(-50%);
  width:400rpx;
  max-height:130rpx;
  overflow:hidden;
}
.receive-coupon-info-left .receive-coupon-condition{
  display:block;
  max-height:80rpx;
  line-height:40rpx;
  text-align:center;
  font-size:28rpx;
  letter-spacing:1rpx;
  word-break:break-all;
  overflow:hidden;
}
.receive-coupon-info-left .receive-coupon-condition>text{
  font-size:32rpx;
}
.receive-coupon-info-left .receive-coupon-time{
  display:block;
  height:36rpx;
  line-height:36rpx;
  text-align:center;
  font-size:24rpx;
}
.receive-coupon-wrapper .receive-coupon .receive-coupon-btn{
  display:block;
  margin:60rpx auto 0;
  width:420rpx;
  height:80rpx;
  font-size:30rpx;
  line-height:80rpx;
  color:#fff;
  letter-spacing: 1rpx;
  background:#0fa8ff;
  border-radius:4rpx;
  text-align:center;
}
.receive-coupon-wrapper .receive-coupon .receive-coupon-close{
  position:absolute;
  top:8rpx;
  right:8rpx;
  width:40rpx;
  height:40rpx;
  border-radius:50%;
  border:1rpx solid #eee;
}
.receive-coupon-wrapper .receive-coupon .receive-coupon-close:before{
  content: '';
  position: absolute;
  height: 1rpx;
  width: 100%;
  top: 50%;
  left: 0;
  margin-top: -1rpx;
  background: #eee;
  transform: rotate(45deg);
}
.receive-coupon-wrapper .receive-coupon .receive-coupon-close:after{
  content: '';
  position: absolute;
  height: 1rpx;
  width: 100%;
  top: 50%;
  left: 0;
  margin-top: -1rpx;
  background: #eee;
  transform: rotate(-45deg);
}